{
  "name": "ESP32-S3 Mini (ESP32-S3-FH4R2)",
  "url": "https://www.waveshare.com/esp32-s3-zero.htm",
  "vendor": "Waveshare",
  "build": {
    "core": "esp32",
    "arduino":{
      "memory_type": "dio_qspi"
    },
    "extra_flags": [
      "-DARDUINO_ESP32S3_DEV",
      "-DARDUINO_USB_CDC_ON_BOOT=1",
      "-DARDUINO_RUNNING_CORE=1",
      "-DARDUINO_EVENT_RUNNING_CORE=1",
      "-DBOARD_HAS_PSRAM"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_size": "4MB",
    "flash_mode": "dio",
    "psram_type": "dio",
    "mcu": "esp32s3",
    "variant": "esp32s3"
  },
  "connectivity": [
    "wifi",
    "bluetooth"
  ],
  "debug": {
    "default_tool": "esp-builtin",
    "onboard_tools": [
      "esp-builtin"
    ],
    "openocd_target": "esp32s3.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 2097152,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "speed": 460800
  },
  "offsets": {
    "bootloader": "0x0",
    "partition_table": "0x8000",
    "application": "0x10000"
  }
}
  